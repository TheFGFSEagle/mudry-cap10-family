   <animation>
    <name>HeliceComplete</name>
    <object-name>helice-ge-tourn</object-name>
    <object-name>helice</object-name>
    <object-name>cone</object-name>
  </animation>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>helice-ge-tourn</object-name>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
  </effect>
  
  <!-- Sélection de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>helice-ge-tourn</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/int[9]</property>
        <value>2700</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>sim/multiplay/generic/int[9]</property>
	<value>400</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>helice</object-name>
    <object-name>cone</object-name>
    <property>sim/multiplay/generic/int[9]</property>
    <factor> 1 </factor>
    <center>
      <x-m> -1.897 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.015 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>helice-ge-tourn</object-name>
    <property>sim/multiplay/generic/int[9]</property>
    <factor> 1 </factor>
    <center>
      <x-m> -1.897 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.015 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

