<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>tachymetre.ac</path>

  <animation>
    <type>range</type>
    <min-m>  0 </min-m>
    <max-m> 45 </max-m>
  </animation>
  
  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>
  
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Cap10B/Models/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>
  
  <!--<animation>
    <type>material</type>
    <object-name>tachymetre</object-name>
    <object-name>aiguille</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>-->
  
  
  <animation>
    <type>rotate</type>
    <object-name>aiguille</object-name>
    <property>engines/engine[0]/rpm</property>
    <interpolation>
      <entry><ind>    0 </ind><dep> -155.458 </dep></entry>
      <entry><ind>  300 </ind><dep> -130.912 </dep></entry>
      <entry><ind>  500 </ind><dep> -114.548 </dep></entry>
      <entry><ind>  800 </ind><dep>  -90.000 </dep></entry> 
      <entry><ind> 1400 </ind><dep>  -40.910 </dep></entry>
      <entry><ind> 1900 </ind><dep>    0.000 </dep></entry>
      <entry><ind> 2400 </ind><dep>   40.910 </dep></entry>
      <entry><ind> 3000 </ind><dep>   90.000 </dep></entry> 
      <entry><ind> 3500 </ind><dep>  130.912 </dep></entry> 
    </interpolation>    
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>

