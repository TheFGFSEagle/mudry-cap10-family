  <!-- manette des gaz -->
  
  <animation>
   <type>translate</type>
   <object-name>manettegaz</object-name>
   <property>controls/engines/engine/throttle</property>
   <factor>0.085</factor>
     <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
     </axis>   
  </animation>

  <animation>
    <type>pick</type>
    <object-name>manettegaz</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.05</interval-sec>
      <binding>
        <command>property-assign</command>
        <property>controls/throttle-coupled</property>
        <value>0</value>
      </binding>
      <binding>
        <command>nasal</command>
        <script>cap10b.set_levers("throttle",0,-1,1);</script>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>controls/throttle-coupled</property>
          <value>1</value>
        </binding>
      </mod-up>
    </action>
  </animation>
  
  <!-- manette de mixture -->
  
  <animation>
   <type>translate</type>
   <object-name>manettemixture</object-name>
   <property>controls/engines/engine/mixture</property>
   <factor>0.085</factor>
     <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
     </axis>   
  </animation>

  <animation>
    <type>pick</type>
    <object-name>manettemixture</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.05</interval-sec>
      <binding>
        <command>property-assign</command>
        <property>controls/mixture-coupled</property>
        <value>0</value>
      </binding>
      <binding>
        <command>nasal</command>
        <script>cap10b.set_levers("mixture",0,-1,1);</script>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>controls/mixture-coupled</property>
          <value>1</value>
        </binding>
      </mod-up>
    </action>
  </animation>
  
  <!-- manette frein de park -->
  <animation>
   <type>translate</type>
   <object-name>manettepark</object-name>
   <property>controls/gear/brake-parking</property>
   <factor>0.03</factor>
     <axis>
      <x> 1</x>
      <y> 0</y>
      <z> 0</z>
     </axis>   
  </animation>

  <animation>
   <type>translate</type>
   <object-name>manettepark</object-name>
   <property>controls/gear/brake-left</property>
   <condition>
     <not><property>controls/gear/brake-parking</property></not>
   </condition>
   <factor>0.015</factor>
     <axis>
      <x> 1</x>
      <y> 0</y>
      <z> 0</z>
     </axis>   
  </animation>

  <animation>
   <type>translate</type>
   <object-name>manettepark</object-name>
   <property>controls/gear/brake-right</property>
   <condition>
     <and>
       <not><property>controls/gear/brake-parking</property></not>
     </and>
   </condition>
   <factor>0.015</factor>
     <axis>
      <x> 1</x>
      <y> 0</y>
      <z> 0</z>
     </axis>   
  </animation>

  <animation>
    <type>pick</type>
    <object-name>manettepark</object-name>
    <action>
      <name>park on</name>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/gear/brake-parking</property>
      </binding>
    </action>
  </animation>

  <!-- Stall alarm -->
  <animation>
    <type>material</type>
    <object-name>alarmedecrochage</object-name>
    <condition>
      <and>
        <greater-than>
          <property>fdm/jsbsim/aero/alpha-wing-rad</property>
          <value>0.279</value>
        </greater-than>
        <not>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </not>
      </and>
    </condition>
    <emission>
      <red>   0.8 </red>
      <green> 0.2 </green>
      <blue>  0.0 </blue>
      <factor-prop>systems/electrical/volts</factor-prop>
    </emission>
  </animation>

  <!-- starter -->
  <animation>
   <type>pick</type>
   <object-name>starter</object-name>
   <visible>true</visible>
   <action>
     <name>starter</name>
     <button>0</button>
     <binding>
       <command>property-assign</command>
       <property>/controls/engines/engine/starter_cmd</property>
       <value>1</value>
       <condition>
         <not-equals>
           <property>controls/fuel/selected-tank</property>
           <value>0</value>
         </not-equals>
       </condition>
     </binding>
     <mod-up>
       <binding>
         <command>property-assign</command>
         <property>/controls/engines/engine/starter_cmd</property>
         <value>0</value>
       </binding>
     </mod-up>
   </action>
  </animation>

  <animation> 
    <type>translate</type>
    <object-name>starter</object-name>
    <property>controls/engines/engine/starter_cmd-pos</property>
    <factor>-0.012</factor>
    <axis> 
      <x>  1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Sélecteur carburant -->
  <animation>
    <type>pick</type>
    <object-name>selecteurcarburant</object-name>
   <action>
     <name>decrease mags: click</name>
     <button>0</button>
     <binding>
       <command>property-adjust</command>
       <property>controls/fuel/selected-tank</property>
       <step>1</step>
       <min>-1</min>
       <max>2</max>
       <wrap>true</wrap>
     </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/click</property>
      </binding>
   </action>
   <action>
     <name>decrease mags: scroll</name>
     <button>3</button>
     <binding>
       <command>property-adjust</command>
       <property>controls/fuel/selected-tank</property>
       <step>1</step>
       <min>-1</min>
       <max>1</max>
     </binding>
   </action>
   <action>
     <name>increase mags: scroll</name>
     <button>4</button>
     <binding>
       <command>property-adjust</command>
       <property>controls/fuel/selected-tank</property>
       <step>-1</step>
       <min>-1</min>
       <max>1</max>
     </binding>
   </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>selecteurcarburant</object-name>
    <property>controls/fuel/selected-tank-pos</property>
    <factor> -90 </factor>
    <center>
      <x-m> -0.2805 </x-m>
      <y-m>  0.0000 </y-m>
      <z-m> -0.1011 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- TEXT OSG -->
